@using HWYZ.Models;
<div class="table-responsive" id="datatable">
    <table class="table table-hover table-bordered">
        <thead>
            <tr>
                <td width="50%">采购单位</td>
                <td>金额</td>
            </tr>
        </thead>
        <tbody>
            @{
                foreach (PSJE item in ViewBag.list)
                {
                <tr>
                    <td>@item.StoreName</td>
                    <td>@item.Pay</td>
                </tr>
                }
            }
        </tbody>
    </table>
</div>

<div class="text-right" style="padding: 10px 15px;">
    @Html.Partial("~/Views/Shared/_pager.cshtml")
</div>

<div id="chart">
</div>
