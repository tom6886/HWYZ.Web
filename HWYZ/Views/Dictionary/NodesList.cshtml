@using HWYZ.Models;

<div class="table-responsive">
    <table class="table table-hover table-bordered">
        <thead>
            <tr>
                <td>名称</td>
                <td>值</td>
                <td>操作</td>
            </tr>
        </thead>
        <tbody>
            @{
                foreach (Dictionary item in Model)
                {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Code</td>
                    <td data-id="@item.ID">
                        <a href="javascript:void(0)" class="editDict">编辑</a>
                    </td>
                </tr>
                }
            }
        </tbody>
    </table>
</div>
